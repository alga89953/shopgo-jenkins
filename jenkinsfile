pipeline{
            
            agent any 
            stages{
                stage('Fetch'){ 
                    steps{
                        sh"dart pub get" 
                    }
                } 
                stage('Build'){
                    steps{
                        sh"dart run lib/main.dart"
                    }
                }
                stage('Lint'){
                    steps{
                        sh"dart analyze"
                    }
                }
                stage('Test'){
                    steps{
                        sh"flutter test"
                    }
                }
                stage('Clean'){
                    steps{
                        cleanWs deleteDirs: true, notFailBuild: true            
                    }
                } 
            }
        }
