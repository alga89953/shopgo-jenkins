// // //Configuracion de flutter app movil
pipeline {
    agent any

    stages {
        stage('Preparación') {
            steps {
                sh 'flutter pub get'
            }
        }

        stage('Compilación') {
            steps {
                sh 'fastlane build'
            }
            //clear
        }

        stage('Pruebas') {
            steps {
                sh 'fastlane test'
            }
        }

        stage('Deploy') {
            steps {
                sh 'fastlane deploy'
            }
        }
    }
}